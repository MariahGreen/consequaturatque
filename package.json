{
  "name": "consequaturatque",
  "version": "1.0.0",
  "description": "Dependency injection container without dependencies",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "format": "prettier 'src/**/*.ts' --write",
    "build": "tsc",
    "prerelease": "rm -rf ./dist",
    "release": "NODE_ENV=production tsc --build tsconfig.json",
    "postrelease": "minify-all-js ./dist",
    "deploy": "npm publish --access public"
  },
  "keywords": [
    "nemo",
    "dependency",
    "injection",
    "ioc",
    "cum",
    "javascript",
    "quo"
  ],
  "homepage": "https://github.com/MariahGreen/consequaturatque.git",
  "author": "GaetanoBeatty",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "jest": "^27.0.6",
    "lint-staged": "^11.2.0",
    "minify-all-js": "^0.1.9",
    "prettier": "^2.4.1",
    "tcr": "^0.5.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MariahGreen/consequaturatque.git"
  },
  "bugs": {
    "url": "https://github.com/MariahGreen/consequaturatque.git/issues"
  }
}